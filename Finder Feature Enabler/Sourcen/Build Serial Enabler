set PATHDEV to "Macintosh HD:Desktop Folder:Finder Feature Enabler:"with timeout of 60000 seconds	OPERATE(PATHDEV & "Finder Feature INIT.µ")	OPERATE(PATHDEV & "Finder Feature Enabler.µ")end timeouton OPERATE(project)	tell application "CodeWarrior IDE 1.6"		open {project}		Make Project	end tellend OPERATE